{
    "private": true,
    "productName": "Entry",
    "name": "entry",
    "version": "2.0.13",
    "description": "Entry for offline",
    "main": "src/main_build/main.bundle.js",
    "scripts": {
        "start": "electron . --debug",
        "debug": "electron --inspect .",
        "webpack:dev": "cross-env NODE_ENV=development webpack",
        "webpack:prod": "cross-env NODE_ENV=production webpack",
        "webpack:main": "cross-env NODE_ENV=development webpack --config webpack.main.config.js",
        "webpack:renderer": "cross-env NODE_ENV=development webpack --config webpack.renderer.config.js",
        "watch": "cross-env NODE_ENV=development webpack -w",
        "rebuild": "electron-rebuild",
        "dist:win32": "cross-env NODE_ENV=production webpack && electron-builder -c --win --ia32",
        "dist:win64": "cross-env NODE_ENV=production webpack && electron-builder -c --win --x64",
        "dist:mac": "cross-env NODE_ENV=production webpack && electron-builder -c --mac",
        "postinstall": "electron-rebuild && bower i"
    },
    "keywords": [
        "entry",
        "entry-offline"
    ],
    "author": "entrylabs",
    "license": "MIT",
    "dependencies": {
        "@babel/runtime": "^7.2.0",
        "@entrylabs/bindings": "1.0.0",
