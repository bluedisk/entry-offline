        "react-dom": "^16.7.0",
        "react-redux": "^7.1.0",
        "redux": "^4.0.1",
        "redux-actions": "^2.6.5",
        "redux-persist": "^5.10.0",
        "rimraf": "^2.6.3",
        "socket.io": "^2.2.0",
